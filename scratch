return (
      this.props.listings.map((listing) => {
        return (
          <div className="list_preview">
            <NavLink to={`listings/${listing.id}`} className="lt"> {listing.title} </NavLink>
            <p>{listing.description}</p>
            <p>{listing.renterName}</p>
            <img src={listing.pictures} alt="" />
          </div>
        );
      })
    );